<%= form_for([:admin, @user]) do |f| %>

  <%= render "common/errors", object: @user %>

  <div class="field">
    <%= f.label :email, 'E-mail:' %>
    <%= f.text_field :email, class: "form-control" %>
  </div>
  <div class="actions">
    <%= f.submit :class =>"btn btn-primary btn-sm" %>
  </div>
<% end %>
